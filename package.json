{
  "name": "posthog-3000-apps",
  "version": "98.0.1998",
  "description": "Monorepo for PostHog 3000 Electron applications",
  "private": true,
  "author": "PostHog Community",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter installer dev",
    "build": "pnpm --filter installer build",
    "typecheck": "pnpm -r typecheck",
    "build:all": "pnpm -r build",
    "dev:installer": "pnpm --filter installer dev",
    "build:installer": "pnpm --filter installer build",
    "build:installer:mac": "pnpm run build:launcher:mac && pnpm --filter installer build:mac",
    "build:installer:win": "pnpm --filter installer build:win",
    "build:installer:linux": "pnpm --filter installer build:linux",
    "dev:launcher": "pnpm --filter launcher dev",
    "build:launcher": "pnpm --filter launcher build",
    "build:launcher:mac": "pnpm --filter launcher build:mac",
    "build:launcher:win": "pnpm --filter launcher build:win",
    "build:launcher:linux": "pnpm --filter launcher build:linux"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@9.15.5+sha256.8472168c3e1fd0bff287e694b053fccbbf20579a3ff9526b6333beab8df65a8d"
}
